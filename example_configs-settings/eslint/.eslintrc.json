{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "quotes": [
      "error",
      "double",
      {
        "avoidEscape": true
      }
    ],
    "import/no-extraneous-dependencies": ["off"],
    "import/no-unresolved": ["off"],
    "import/extensions": ["off"],
    "import/no-cycle": ["off"],
    "no-unused-expressions": ["off"],
    "no-sequences": ["off"],
    "no-console": ["off"],
    "func-names": ["off"]
  }
}
